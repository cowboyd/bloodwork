<h1>Patients</h1>
<table class="table table-striped">
  <thead>
    <tr><th>Name</th><th>Age</th><th>Gender</th></tr>
  </thead>
  <tbody>
    {{#each model as |patient|}}
      <tr>
        <td>{{patient.name}}</td>
        <td>{{patient.age}}</td>
        <td>{{patient.gender}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
