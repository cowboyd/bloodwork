<h1>Patients</h1>
<table class="table table-striped table-hover">
  <thead>
    <tr><th>Name</th><th>Age</th><th>Gender</th></tr>
  </thead>
  <tbody>
    {{#each model as |patient|}}
      {{#link-to "patients.patient" patient tagName="tr"}}
        <td>{{patient.name}}</td>
        <td>{{patient.age}}</td>
        <td>{{patient.gender}}</td>
      {{/link-to}}
    {{/each}}
  </tbody>
</table>
