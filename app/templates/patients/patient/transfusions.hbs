<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>#</th>
      <th>Date</th>
      <th>Units</th>
    </tr>
  </thead>
  <tbody>
    {{#each model.transfusions as |transfusion index|}}
      <tr>
        <td>{{index}}</td>
        <td>{{moment-format transfusion.date "MMM DD YYYY"}}</td>
        <td>{{transfusion.units}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>
